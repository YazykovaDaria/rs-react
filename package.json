{
  "name": "rs-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon server/server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc && vite build --outDir dist/client",
    "build:server": " tsc && vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:cy": "npx start-server-and-test dev http-get://localhost:7456 cy:run && npx nyc report && npx nyc report --reporter=text-summary",
    "cy:run": "cypress run --e2e",
    "cy:open": "cypress open --e2e --browser chrome",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "prepare": "husky install",
    "clean": "rimraf dist/"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "reset-css": "^5.0.1",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/concurrently": "^7.0.0",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "cypress": "^12.11.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "msw": "^1.2.1",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "rimraf": "^5.0.0",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.2"
  },
  "lint-staged": {
    "*.(tsx)": [
      "npm run lint-fix"
    ]
  }
}
